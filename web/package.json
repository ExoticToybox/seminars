{
  "name": "web",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "yarn lint --fix",
    "format": "yarn prettier --write './**/*.{js,jsx,ts,tsx,json}'",
    "format:fix": "yarn format && yarn lint:fix",
    "graphql:generate": "yarn graphql-codegen --config codegen.config.ts --require dotenv/config",
    "dev": "vite",
    "test": "yarn vitest --run",
    "test:watch": "yarn vitest --watch",
    "build": "tsc && vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "@graphql-codegen/typescript": "^2.8.7",
    "@graphql-codegen/typescript-operations": "^2.5.12",
    "@graphql-codegen/typescript-react-query": "^4.0.6",
    "@graphql-typed-document-node/core": "^3.1.1",
    "@headlessui/react": "^1.6.4",
    "@heroicons/react": "^2.0.13",
    "@tanstack/react-query": "^4.22.0",
    "@types/react-router-dom": "^5.3.3",
    "graphql": "^16.6.0",
    "graphql-request": "^5.1.0",
    "luxon": "^3.2.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-hook-form": "^7.43.0",
    "react-router-dom": "^6.8.0"
  },
  "devDependencies": {
    "@graphql-codegen/add": "^3.2.3",
    "@graphql-codegen/cli": "^2.16.3",
    "@graphql-codegen/client-preset": "^1.2.5",
    "@types/luxon": "^3.2.0",
    "@types/node": "^18.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "@vitejs/plugin-react": "^3.0.1",
    "autoprefixer": "^10.4.7",
    "better-typescript-lib": "^2.1.0",
    "eslint": "^8.2.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-import-access": "^1.0.1",
    "eslint-plugin-jsx-a11y": "^6.7.0",
    "eslint-plugin-prefer-arrow-functions": "^3.1.4",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "postcss": "^8.4.21",
    "prettier": "^2.8.2",
    "tailwindcss": "^3.1.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4",
    "vite": "^4.0.4",
    "vite-tsconfig-paths": "^4.0.3",
    "vitest": "^0.27.0"
  }
}
